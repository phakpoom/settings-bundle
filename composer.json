{
    "name": "phakpoom/settings-bundle",
    "description": "Just a Symfony settings bundle.",
    "type": "symfony-bundle",
    "keywords": [
        "php",
        "settings",
        "settings-bundle"
    ],
    "homepage": "https://github.com/phakpoom/settings-bundle",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Ishmael Doss",
            "email": "nukboon@gmail.com"
        },
        {
            "name": "Bonn"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpMob\\SettingsBundle\\": "src",
            "tests\\PhpMob\\SettingsBundle\\": "tests"
        },
        "classmap": []
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "require": {
        "php": ">=7.2 || >=8.0",
        "phakpoom/settings": "^2.2.2",
        "symfony/form": "^4.4 | ^5.0 | ^6.0 | ^7.0",
        "symfony/framework-bundle": "^4.4 | ^5.0 | ^6.0 | ^7.0",
        "symfony/templating": "^4.4 | ^5.0 | ^6.0 | ^7.0",
        "twig/twig": "^2.0|^3.0"
    },
    "require-dev": {
        "cache/filesystem-adapter": "^1.0",
        "phpspec/phpspec": "^7.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "cache/filesystem-adapter": "To use built-in \\PhpMob\\Settings\\Manager\\CachedManager."
    }
}
